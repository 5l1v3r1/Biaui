<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:biaui="clr-namespace:Biaui"
                    xmlns:controls="clr-namespace:Biaui.Controls">
    <Style TargetType="{x:Type controls:BiaUniformGrid}">
        <Setter Property="ColumnSpacing" Value="{x:Static biaui:Boxes.Double1}" />
        <Setter Property="RowSpacing" Value="{x:Static biaui:Boxes.Double1}" />
    </Style>
</ResourceDictionary>